#include <iostream>
using namespace std;

int main() {
    int n;
    cin >> n;
    int fullSum = 0;
    for (int i = 0; i < n; i++) {
        int x;
        cin >> x;
        fullSum += x;
    }
    int partialSum = 0;
    for (int i = 0; i < n - 1; i++) {
        int x;
        cin >> x;
        partialSum += x;
    }
    cout << fullSum - partialSum;
    
    return 0;
}
